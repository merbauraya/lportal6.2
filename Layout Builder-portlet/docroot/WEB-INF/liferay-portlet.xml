<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>Layout Builder</portlet-name>
		<icon>/icon.png</icon>
		<control-panel-entry-category>server</control-panel-entry-category>
		<control-panel-entry-weight>900</control-panel-entry-weight>
		<instanceable>false</instanceable>
		<requires-namespaced-parameters>false</requires-namespaced-parameters>
		<header-portlet-css>/js/jquery-ui-1.11.2.custom/jquery-ui.min.css</header-portlet-css>
		<header-portlet-css>/css/jquery.contextmenu.css</header-portlet-css>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/jquery-1.11.1.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/jquery-ui-1.11.2.custom/jquery-ui.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/jquery.contextmenu.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/jquery.format.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/html2canvas.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/jquery.fileDownload.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>Layout Builder-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>